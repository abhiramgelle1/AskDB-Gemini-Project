# Google AI (required) - https://aistudio.google.com/apikey
GOOGLE_API_KEY=your_google_api_key_here
# If 429 quota: try GEMINI_MODEL=gemini-3-flash-preview or set up billing in AI Studio
GEMINI_MODEL=gemini-2.0-flash
# Timeout in seconds (default 90; increase if you get 504 DEADLINE_EXCEEDED)
GEMINI_TIMEOUT=90

# LangChain (optional - set false to skip)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=askdb_project
LANGCHAIN_API_KEY=

# PostgreSQL - your ogms database
DB_TYPE=postgresql
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ogms
DB_USER=postgres
DB_PASSWORD=your_password
